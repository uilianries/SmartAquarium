<?xml version="1.0" encoding="UTF-8" ?>
<config>
<thermal_sensor>
    <pin>UART4_RXD</pin>
	<mqtt>
		<server>m11.cloudmqtt.com</server>
		<port>15347</port>
		<clientid>ThermalSensor</clientid>
		<username>thermal_sensor</username>
		<password>thermalsensor</password>
		<topic>smartaquarium/sensor/temperature/+</topic>
		<actuatortopic>smartaquarium/heater</actuatortopic>
	</mqtt>
	<breed>
		<betta>
			<min>22</min>
			<max>30</max>
		</betta>
	</breed>
</thermal_sensor>
</config>
